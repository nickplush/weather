atherDay}</div>\n            <div>\n                <YMaps>\n                    <Map defaultState={mapData}>\n                        {coordinates.map(coordinate => <Placemark geometry={coordinate} />)}\n                    </Map>\n                </YMaps>\n            </div>\n        </div>\n    )\n}\n","/home/niko/work/saveWeather/my-app/src/components/Weather/Home.js",[],{"ruleId":"70","severity":1,"message":"71","line":21,"column":36,"nodeType":"72","endLine":21,"endColumn":38,"suggestions":"73"},{"ruleId":"70","severity":1,"message":"74","line":38,"column":47,"nodeType":"72","endLine":38,"endColumn":59,"suggestions":"75"},{"ruleId":"76","severity":1,"message":"77","line":12,"column":9,"nodeType":"78","messageId":"79","endLine":28,"endColumn":10},{"ruleId":"80","severity":1,"message":"81","line":1,"column":1,"nodeType":"82","endLine":8,"endColumn":2},{"ruleId":"80","severity":1,"message":"81","line":1,"column":1,"nodeType":"82","endLine":14,"endColumn":2},{"ruleId":"83","replacedBy":"84"},{"ruleId":"85","replacedBy":"86"},{"ruleId":"70","severity":1,"message":"87","line":47,"column":40,"nodeType":"72","endLine":47,"endColumn":52,"suggestions":"88"},{"ruleId":"70","severity":1,"message":"89","line":15,"column":40,"nodeType":"72","endLine":15,"endColumn":52,"suggestions":"90"},{"ruleId":"70","severity":1,"message":"89","line":15,"column":40,"nodeType":"72","endLine":15,"endColumn":52,"suggestions":"91"},"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'getMyLocation'. Either include it or remove the dependency array.","ArrayExpression",["92"],"React Hook useEffect has a missing dependency: 'getWeatherOfMyLocation'. Either include it or remove the dependency array.",["93"],"default-case","Expected a default case.","SwitchStatement","missingDefaultCase","import/no-anonymous-default-export","Unexpected default export of anonymous function","ExportDefaultDeclaration","no-native-reassign",["94"],"no-negated-in-lhs",["95"],"React Hook useEf